{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "off",
    "no-console": "off",
    "prefer-const": "warn",
    "no-var": "warn",
    "react/no-unescaped-entities": "off",
    "@next/next/no-img-element": "off"
  },
  "ignorePatterns": [".next/", "node_modules/", "dist/"]
}
